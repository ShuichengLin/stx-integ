    <method name="GET" id="getProvidernetrange">
        <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN"
            title="Show provider network range">
            <para role="shortdesc">Shows detailed information about a specific provider network range.</para>
