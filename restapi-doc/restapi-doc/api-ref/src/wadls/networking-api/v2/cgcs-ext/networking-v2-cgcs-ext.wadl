<!DOCTYPE application [<!ENTITY % common  SYSTEM "../common.ent">
%common;]>
<application xmlns="http://wadl.dev.java.net/2009/02"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:xsdxt="http://docs.rackspacecloud.com/xsd-ext/v1.0"
    xmlns:wadl="http://wadl.dev.java.net/2009/02">
<!-- REMOVED xmlns:csapi="http://docs.openstack.org/networking/api/v2" -->


    <!--*******************************************************-->
    <!--               RESOURCES                               -->
    <!--*******************************************************-->

    <resources base="https://windriver.com/cgcs-restapi/neutron/"
        xml:id="networking-v2-cgcs-ext">

        <!-- /v2.0                                               -->
        <resource id="version" type="#VersionDetails" path="//v2.0">

        <!-- /v2/extensions                                    -->
            <resource id="extensions" path="extensions">
                <method href="#listExtensions"/>
        <!-- /v2/extensions/{extension_alias}                  -->
                 <resource path="{extension_alias}" id="extensionget">
                     <param name="extension_alias" style="template"
                         type="xsd:string">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook"
                            xml:lang="EN"><para>The alias for the extension 
                                to list.</para>
                        </wadl:doc>
                     </param>
                     <method href="#getExtension"/>
                 </resource>
            </resource>

        <!-- /v2.0/wrs-provider                                  -->
            <resource id="wrs-provider" path="wrs-provider">

        <!-- /v2.0/wrs-provider/providernets                                  -->
                <resource id="providernets" path="providernets">
                    <method href="#listProvidernets"/>
                    <method href="#createProvidernet"/>

        <!-- /v2.0/wrs-provider/providernets/{providernet_id}                 -->
                    <resource id="providernet_id" path="{providernet_id}">
                        <param name="providernet_id" style="template" type="csapi:UUID">
                            <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                                <para>The ID for a provider network.</para>
                            </wadl:doc>
                        </param>
                        <method href="#getProvidernet"/>
                        <method href="#updateProvidernet"/>
                        <method href="#deleteProvidernet"/>

        <!-- /v2.0/wrs-provider/providernets/{providernet_id}/providernet-bindings  -->
                        <resource id="providernet_bindings" path="providernet-bindings">
                          <method href="#listNetworksOnProviderNet"/>
                        </resource>
                    </resource>
                </resource>


        <!-- /v2.0/wrs-provider/providernet-ranges                                  -->
                <resource id="providernetranges" path="providernet-ranges">
                    <method href="#listProvidernetranges"/>
                    <method href="#createProvidernetrange"/>

        <!-- /v2.0/wrs-provider/providernet-ranges/{providernet-range_id}                 -->
                    <resource id="providernetrange_id" path="{providernet-range_id}">
                        <param name="providernetrange_id" style="template" type="csapi:UUID">
                            <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                                <para>The ID for a provider network segmentation range.</para>
                            </wadl:doc>
                        </param>
                        <method href="#getProvidernetrange"/>
                        <method href="#updateProvidernetrange"/>
                        <method href="#deleteProvidernetrange"/>
                    </resource>
                </resource>


        <!-- /v2.0/wrs-provider/providernet-types                                  -->
                <resource id="providernettypes" path="providernet-types">
                    <method href="#listProvidernettypes"/>
                </resource>

        <!-- /v2.0/wrs-provider/providernet-connectivity-tests                                  -->
                <resource id="providernetconnectivitytests" path="providernet-connectivity-tests">
                    <method href="#listProvidernetconnectivitytests"/>
                    <method href="#scheduleProvidernetconnectivitytest"/>
                </resource>

            </resource>

        <!-- /v2.0/hosts                                  -->
            <resource id="hosts" path="hosts">
                <method href="#listHosts"/>
                <method href="#createHost"/>

        <!-- /v2.0/hosts/{host_id}                 -->
                <resource id="host_id" path="{host_id}">
                    <param name="host_id" style="template" type="csapi:UUID">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                            <para>The ID for a host running neutron services.</para>
                        </wadl:doc>
                    </param>
                    <method href="#getHost"/>
                    <method href="#updateHost"/>
                    <method href="#deleteHost"/>

                </resource>
            </resource>


        <!-- /v2.0/wrs-tenant                                           -->
            <resource id="wrs-tenant" path="wrs-tenant">
        <!-- /v2.0/wrs-tenant/settings                                  -->
                <resource id="settings" path="settings">
                    <method href="#listSettings"/>
        <!-- /v2.0/wrs-tenant/settings/{tenant_id}                 -->
                    <resource id="tenant_id" path="{tenant_id}">
                        <param name="tenant_id" style="template" type="csapi:UUID">
                            <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                                <para>The ID for a tenant.</para>
                            </wadl:doc>
                        </param>
                        <method href="#getSetting"/>
                        <method href="#updateSetting"/>
                        <method href="#deleteSetting"/>
                    </resource>

                </resource>

            </resource>


        <!-- /v2.0/wrs-tm                                 -->
            <resource id="wrs-tm" path="wrs-tm">

        <!-- /v2.0/wrs-tm/qoses                                  -->
                <resource id="qoses" path="qoses">
                    <method href="#listQoses"/>
                    <method href="#createQos"/>

        <!-- /v2.0/wrs-tm/qoses/{qos_id}                 -->
                    <resource id="qos_id" path="{qos_id}">
                        <param name="qos_id" style="template" type="csapi:UUID">
                            <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                                <para>The ID for a QOS Policy.</para>
                            </wadl:doc>
                        </param>
                        <method href="#getQos"/>
                        <method href="#updateQos"/>
                        <method href="#deleteQos"/>
                    </resource>

                </resource>

            </resource>


        <!-- /v2.0/networks                              -->
            <resource id="networks" path="networks">
                <method href="#listNetworks"/>
                <method href="#createNetwork"/>

        <!-- /v2.0/networks/{network_id}                  -->
                <resource path="{network_id}" id="network_id">
                    <param name="network_id" style="template" type="csapi:UUID">
                        <wadl:doc
                            xmlns="http://docbook.org/ns/docbook"
                            xml:lang="EN" title="Network ID">
                            <para>The UUID for a network.</para>
                        </wadl:doc>
                    </param>
                    <method href="#showNetwork"/>

                </resource>
            </resource>


        <!-- /v2.0/subnets                              -->
            <resource id="subnets" path="subnets">
                <method href="#listSubnets"/>
                <method href="#createSubnet"/>

        <!-- /v2.0/subnets/{subnet_id}                  -->
                <resource path="{subnet_id}" id="subnet_id">
                    <param name="subnet_id" style="template" type="csapi:UUID">
                        <wadl:doc
                            xmlns="http://docbook.org/ns/docbook"
                            xml:lang="EN" title="Subnet ID">
                            <para>The UUID for a subnet.</para>
                        </wadl:doc>
                    </param>
                    <method href="#getSubnet"/>

                </resource>
            </resource>


        <!-- /v2.0/ports                                -->
            <resource id="ports" path="ports">
                <method href="#listPorts"/>

        <!-- /v2.0/ports/{port_id}                      -->
                <resource path="{port_id}" id="port_id">
                    <param name="port_id" style="template"
                        type="csapi:UUID">
                        <wadl:doc
                            xmlns="http://docbook.org/ns/docbook"
                            xml:lang="EN" title="Port ID">
                            <para>The UUID for a port.</para>
                        </wadl:doc>
                    </param>
                    <method href="#showPort"/>

                </resource>
            </resource>


        <!-- /v2.0/routers                              -->
            <resource id="routers" path="routers">
                <method href="#listRouters"/>

        <!-- /v2.0/routers/{router_id}                  -->
                <resource path="{router_id}" id="router_id">
                    <param
                        xmlns="http://wadl.dev.java.net/2009/02"
                        name="router_id" style="template"
                        type="csapi:UUID">
                        <wadl:doc
                            xmlns="http://docbook.org/ns/docbook"
                            xml:lang="EN" title="Router ID">
                            <para>The UUID for a router.</para>
                        </wadl:doc>
                    </param>
                    <method href="#showRouter"/>

                </resource>
            </resource>

        <!-- /v2.0/portforwarding                  -->
            <resource id="portforwardings" path="portforwardings">
                <method href="#listPortForwardings"/>
                <method href="#createPortForwarding"/>

        <!-- /v2.0/portforwardings/{portforwarding_id}                 -->
                <resource id="portforwarding_id" path="{portforwarding_id}">
                    <param name="portforwarding_id" style="template" type="csapi:UUID">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                            <para>The ID for a virtual router port forwarding rule.</para>
                        </wadl:doc>
                    </param>
                    <method href="#showPortForwarding"/>
                    <method href="#updatePortForwarding"/>
                    <method href="#deletePortForwarding"/>

                </resource>
            </resource>

        </resource>
    </resources>


    <!--*******************************************************-->
    <!--               METHODS                                 -->
    <!--*******************************************************-->

    <!-- ..................................................... -->
    <!-- ..  Extensions                                     .. -->
    <!-- ..................................................... -->

    <method name="GET" id="listExtensions">
        <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="List extensions">
            <para role="shortdesc">Lists all extensions.</para>
            <para></para>
        </wadl:doc>
        <response status="200">
            <representation mediaType="application/xml">
                <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                </wadl:doc>
                &extensionListShowParameters;
            </representation>
            <representation mediaType="application/json">
                <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                    <xsdxt:code href="../api_samples/networking-v2-cgcs-ext/extension_list-response.json"/>
                </wadl:doc>
            </representation>
        </response>
        &commonFaults; &getFaults;
    </method>

    <method name="GET" id="getExtension">
        <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="Get extension">
            <para role="shortdesc">Gets information about a specified extension.</para>
            <para></para>
        </wadl:doc>
        <response status="200">
            <representation mediaType="application/xml">
                <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                </wadl:doc>
                &extensionListShowParameters;
            </representation>
            <representation mediaType="application/json">
                <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                    <xsdxt:code href="../api_samples/networking-v2-cgcs-ext/extension_get-response.json"/>
                </wadl:doc>
            </representation>
        </response>
        &commonFaults; &getFaults;
    </method>


    <!-- ..................................................... -->
    <!-- ..  Provider Nets                                  .. -->
    <!-- ..................................................... -->

    <method name="GET" id="listProvidernets">
        <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="List provider networks">
            <para role="shortdesc">Lists all provider networks.</para>
        </wadl:doc>
        <response status="200">
            <representation mediaType="application/xml">
                <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                </wadl:doc>
                <param xmlns="http://wadl.dev.java.net/2009/02"
                    name="providernets" style="plain" type="xsd:list" >
                    <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xmlns:wadl="http://wadl.dev.java.net/2009/02" xml:lang="EN">
                        <para>The list of provider networks.</para>
                    </wadl:doc>
                </param>
                &providernetListShowParameters;
            </representation>
            <representation mediaType="application/json">
                <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                    <xsdxt:code href="../api_samples/networking-v2-cgcs-ext/providernet-list_admin_response.json"/>
                </wadl:doc>
            </representation>
        </response>
        &commonFaults; &getFaults;
    </method>


    <method name="GET" id="getProvidernet">
        <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN"
            title="Show provider network">
            <para role="shortdesc">Shows detailed information about a specific provider network.</para>
        </wadl:doc>
        <response status="200">
            <representation mediaType="application/xml">
                <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                </wadl:doc>
                &providernetListShowParameters;
            </representation>
            <representation mediaType="application/json">
                <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                    <xsdxt:code href="../api_samples/networking-v2-cgcs-ext/providernet-show_admin_response.json"/>
                </wadl:doc>
            </representation>
        </response>
        &commonFaults; &getFaults;
    </method>


    <method name="PUT" id="updateProvidernet">
        <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN"
            title="Modify provider network">
            <para role="shortdesc">Modifies a specific provider network.</para>
        </wadl:doc>
        <request>
            <representation mediaType="application/xml">
                <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                </wadl:doc>
                <param xmlns="http://wadl.dev.java.net/2009/02"
                    name="description" style="plain" type="xsd:string" >
                    <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xmlns:wadl="http://wadl.dev.java.net/2009/02" xml:lang="EN">
                        <para>User defined description of the provider network.</para>
                    </wadl:doc>
                </param>
                <param xmlns="http://wadl.dev.java.net/2009/02"
                    name="vlan_transparent" style="plain" type="xsd:bool" >
                    <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xmlns:wadl="http://wadl.dev.java.net/2009/02" xml:lang="EN">
                        <para>Specifies whether VLAN transparent network are supported on this provider network.</para>
                    </wadl:doc>
                </param>
            </representation>
            <representation mediaType="application/json">
                <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                    <xsdxt:code href="../api_samples/networking-v2-cgcs-ext/providernet-update_admin_request.json"/>
                </wadl:doc>
            </representation>
        </request>
        <response status="200">
            <representation mediaType="application/xml">
                <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                </wadl:doc>
                &providernetListShowParameters;
            </representation>
            <representation mediaType="application/json">
                <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                    <xsdxt:code href="../api_samples/networking-v2-cgcs-ext/providernet-update_admin_response.json"/>
                </wadl:doc>
            </representation>
        </response>
        &postPutFaults;
    </method>


    <method name="POST" id="createProvidernet">
        <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN"
            title="Create provider network">
            <para role="shortdesc">Creates a provider network.</para>
        </wadl:doc>
        <request>
            <representation mediaType="application/xml">
                <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                </wadl:doc>
                <param xmlns="http://wadl.dev.java.net/2009/02"
                    name="description" style="plain" type="xsd:string" >
                    <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xmlns:wadl="http://wadl.dev.java.net/2009/02" xml:lang="EN">
                        <para>User defined description of the provider network.</para>
                    </wadl:doc>
                </param>
                <param xmlns="http://wadl.dev.java.net/2009/02"
                    name="name" style="plain" type="xsd:string" >
                    <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xmlns:wadl="http://wadl.dev.java.net/2009/02" xml:lang="EN">
                        <para>The user defined name of the provider network.</para>
                    </wadl:doc>
                </param>
                <param xmlns="http://wadl.dev.java.net/2009/02"
                    name="type" style="plain" type="xsd:string" >
                    <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xmlns:wadl="http://wadl.dev.java.net/2009/02" xml:lang="EN">
                        <para>The encapsulation type of the provider network.  Valid values are: <code>vlan</code>, <code>flat</code></para>
                    </wadl:doc>
                </param>
                <param xmlns="http://wadl.dev.java.net/2009/02"
                    name="vlan_transparent" style="plain" type="xsd:bool" >
                    <wadl:doc xmlns="http://docbook.org/ns/docbook"
                        xmlns:wadl="http://wadl.dev.java.net/2009/02" xml:lang="EN">
                        <para>Specifies whether VLAN transparent network are supported on this provider network.</para>
                    </wadl:doc>
                </param>
            </representation>
            <representation mediaType="application/json">
                <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                    <xsdxt:code href="../api_samples/networking-v2-cgcs-ext/providernet-create_admin_request.json"/>
                </wadl:doc>
            </representation>
        </request>
        <response status="200">
            <representation mediaType="application/xml">
                <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                </wadl:doc>
                &providernetListShowParameters;
            </representation>
            <representation mediaType="application/json">
                <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                    <xsdxt:code href="../api_samples/networking-v2-cgcs-ext/providernet-create_admin_response.json"/>
                </wadl:doc>
            </representation>
        </response>
        &postPutFaults;
    </method>


    <method name="DELETE" id="deleteProvidernet">
        <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN"
            title="Delete provider network">
            <para role="shortdesc">Deletes a specific provider network.</para>
        </wadl:doc>
        <response status="204">
            <representation mediaType="application/json">
                <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                </wadl:doc>
            </representation>
        </response>
    </method>



